{
  "name": "lingo",
  "displayName": "Lingo",
  "publisher": "lingo",
  "version": "0.2.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/vilterp/datalog-ts"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "datalog",
        "extensions": [
          ".dl"
        ],
        "configuration": "./dl-language-configuration.json"
      },
      {
        "id": "datalog2",
        "extensions": [
          ".dl2"
        ],
        "configuration": "./dl2-language-configuration.json"
      },
      {
        "id": "grammar",
        "extensions": [
          ".grammar"
        ],
        "configuration": "./grammar-language-configuration.json"
      },
      {
        "id": "basicBlocks",
        "extensions": [
          ".bb"
        ],
        "configuration": "./basic-blocks-language-configuration.json"
      },
      {
        "id": "opt",
        "extensions": [
          ".opt"
        ],
        "configuration": "./opt-language-configuration.json"
      }
    ],
    "commands": [
      {
        "command": "datalog.openExplorer",
        "title": "Open Explorer",
        "category": "Datalog"
      }
    ]
  }
}