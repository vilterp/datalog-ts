{
  "name": "datalog",
  "displayName": "Datalog",
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/vilterp/datalog-ts"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "activationEvents": [
    "onLanguage:datalog"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "datalog",
        "extensions": [
          ".dl"
        ],
        "aliases": [
          "Datalog"
        ]
      }
    ],
    "grammars": [
      {
        "language": "datalog",
        "scopeName": "source.datalog",
        "path": "./out/datalog.tmLanguage.json"
      }
    ]
  }
}
