{
  "name": "datalog",
  "displayName": "Datalog",
  "version": "0.0.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/vilterp/datalog-ts"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "activationEvents": [
    "onLanguage:datalog",
    "onLanguage:grammar"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "datalog",
        "extensions": [
          ".dl"
        ]
      },
      {
        "id": "grammar",
        "extensions": [
          ".grammar"
        ]
      }
    ],
    "commands": [
      {
        "command": "datalog.openExplorer",
        "title": "Open Explorer",
        "category": "Datalog"
      }
    ]
  }
}
