{
  "name": "lingo",
  "displayName": "Lingo",
  "publisher": "lingo",
  "version": "0.2.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/vilterp/datalog-ts"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "activationEvents": [
    "onLanguage:datalog",
    "onLanguage:grammar",
    "onLanguage:basicBlocks"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "datalog-ts",
          "title": "DatalogTS",
          "icon": "logo.svg"
        }
      ]
    },
    "views": {
      "datalog-ts": [
        {
          "id": "datalog-ts-facts",
          "name": "Facts"
        },
        {
          "id": "datalog-ts-rules",
          "name": "Rules"
        },
        {
          "id": "datalog-ts-visualizations",
          "name": "Visualizations"
        }
      ]
    },
    "languages": [
      {
        "id": "datalog",
        "extensions": [
          ".dl"
        ],
        "configuration": "./dl-language-configuration.json"
      },
      {
        "id": "grammar",
        "extensions": [
          ".grammar"
        ],
        "configuration": "./grammar-language-configuration.json"
      },
      {
        "id": "basicBlocks",
        "extensions": [
          ".bb"
        ],
        "configuration": "./basic-blocks-language-configuration.json"
      }
    ],
    "commands": [
      {
        "command": "datalog.openExplorer",
        "title": "Open Explorer",
        "category": "Datalog"
      }
    ]
  }
}