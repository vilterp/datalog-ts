redir_edge{from: F, to: T, label: "redirect"} :-
    redirect{from: F, to: T}.
uses_edge{from: F, to: T, label: "uses"} :-
    element_usage{el: F, used_in: T}.
edge{from: F, to: T, label: L} :-
    redir_edge{from: F, to: T, label: L} |
    uses_edge{from: F, to: T, label: L}.

page_node{id: I, name: N} :-
    page{id: I, name: N}.
node{id: I, name: N} :-
    page_node{id: I, name: N}.
