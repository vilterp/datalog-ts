{
  "name": "datalog-ts",
  "version": "0.5.5",
  "scripts": {
    "test": "ts-node allTests.ts",
    "test-stay-alive": "ts-node allTests.ts --stay-alive",
    "test-write-results": "ts-node allTests.ts --write-results",
    "test-build-archive": "ts-node util/ddTest/buildArchive.ts",
    "test-write-and-archive": "yarn test-write-results; yarn test-build-archive",
    "repl": "ts-node cmd/repl.ts",
    "build-js": "tsc --outDir dist cmd/repl.ts && tsc --outDir dist cmd/json2dl.ts && tsc --outDir dist cmd/flatten-fp.ts",
    "build-interp-bundle": "esbuild --bundle --outfile=dist/index.mjs --format=esm --target=es6 interpreter.ts",
    "build-fp-ui": "esbuild fp/ui/main.tsx --bundle --outfile=fp/ui/bundle.js '--define:process.env.NODE_ENV=\"development\"' --loader:.dl=text --loader:.css=text --sourcemap --trace=fp/ui/build.trace --define:process=window --define:global=window --define:process.cwd=String",
    "serve-fp-ui": "cd fp/ui && http-server -c-1",
    "build-fiddle-ui": "esbuild ui/main.tsx --bundle --outfile=ui/bundle.js '--define:process.env.NODE_ENV=\"development\"' --loader:.dl=text --sourcemap",
    "serve-fiddle-ui": "cd ui && http-server -c-1",
    "build-notebook-ui": "esbuild notebook/main.tsx --bundle --outfile=notebook/public/bundle.js '--define:process.env.NODE_ENV=\"development\"' --sourcemap",
    "serve-notebook-ui": "cd notebook/public && http-server -c-1",
    "build-parserlib-playground": "esbuild parserlib/playground/main.tsx --bundle --outfile=parserlib/playground/public/bundle.js '--define:process.env.NODE_ENV=\"development\"' --sourcemap",
    "serve-parserlib-playground": "cd parserlib/playground/public && http-server -c-1",
    "build-ddtest-viewer": "esbuild ddTestViewer/main.tsx --bundle --outfile=ddTestViewer/public/bundle.js '--define:process.env.NODE_ENV=\"development\"' --sourcemap --external:path --external:fs --external:crypto",
    "serve-ddtest-viewer": "cd ddTestViewer && http-server -c-1",
    "build-incr-playground": "esbuild parserlib/incrPlayground/main.ts --bundle --outfile=parserlib/incrPlayground/public/bundle.js '--define:process.env.NODE_ENV=\"development\"' --define:process=window --define:global=window",
    "serve-incr-playground": "cd parserlib/incrPlayground/public && http-server -c-1",
    "cloc": "cloc --vcs=git --exclude-dir=dist",
    "benchmarks": "ts-node allBenchmarks.ts"
  },
  "devDependencies": {
    "@jaegertracing/plexus": "^0.1.0",
    "@react-hook/window-size": "^3.0.7",
    "@types/diff": "^4.0.2",
    "@types/node": "^14.14.13",
    "@types/parsimmon": "^1.10.1",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@types/react-select": "^3.0.28",
    "airtable": "^0.10.1",
    "aws-sdk": "^2.802.0",
    "diff": "^4.0.2",
    "esbuild": "^0.5.18",
    "http-server": "^0.12.3",
    "prettier": "^2.0.4",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "@hookeasy/use-title": "^1.0.2",
    "classnames": "^2.2.6",
    "denque": "^1.4.1",
    "graphviz-react": "^1.1.1",
    "parsimmon": "^1.13.0",
    "path-browserify": "^1.0.1",
    "prettier-printer": "^1.1.4",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "react-json-view": "^1.19.1",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.1",
    "react-simple-code-editor": "https://github.com/vilterp/react-simple-code-editor#a1f205b84348964fba02163d1e52e908613d484b",
    "react-textarea-autosize": "^8.2.0",
    "react-use-localstorage": "^3.4.1",
    "simple-markdown": "^0.7.2",
    "ts-node": "^8.10.1",
    "use-hash-param": "^0.2.3",
    "use-http": "^1.0.13",
    "v8-profiler-node8": "^6.3.0",
    "w3c-hr-time": "^1.0.2"
  },
  "bin": {
    "datalog-ts": "dist/cmd/repl.js",
    "json2dl": "dist/cmd/json2dl.js",
    "flatten-fp": "dist/cmd/flatten-fp.js"
  }
}
