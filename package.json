{
  "name": "datalog-ts",
  "version": "0.3.1",
  "scripts": {
    "test": "ts-node allTests.ts",
    "test-stay-alive": "ts-node allTests.ts --stay-alive",
    "test-write-results": "ts-node allTests.ts --write-results",
    "repl": "ts-node cmd/repl.ts",
    "build": "tsc --outDir dist cmd/repl.ts && tsc --outDir dist cmd/json2dl.ts && tsc --outDir dist cmd/flatten-fp.ts",
    "build-fp-ui": "esbuild fp/ui/main.tsx --bundle --outfile=fp/ui/bundle.js '--define:process.env.NODE_ENV=\"development\"' --loader:.dl=text --minify --sourcemap --trace=fp/ui/build.trace",
    "serve-fp-ui": "cd fp/ui && http-server",
    "build-ui": "esbuild ui/main.tsx --bundle --outfile=ui/bundle.js '--define:process.env.NODE_ENV=\"development\"' --loader:.dl=text --sourcemap --minify"
  },
  "devDependencies": {
    "@types/diff": "^4.0.2",
    "@types/node": "^13.13.0",
    "diff": "^4.0.2",
    "esbuild": "^0.3.9",
    "http-server": "^0.12.3",
    "prettier": "^2.0.4"
  },
  "dependencies": {
    "@jaegertracing/plexus": "^0.1.0",
    "@types/parsimmon": "^1.10.1",
    "parsimmon": "^1.13.0",
    "prettier-printer": "^1.1.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  },
  "bin": {
    "datalog-ts": "dist/cmd/repl.js",
    "json2dl": "dist/cmd/json2dl.js",
    "flatten-fp": "dist/cmd/flatten-fp.js"
  }
}
