{
  "name": "datalog-ts",
  "version": "0.5.9",
  "scripts": {
    "test": "ts-node allTests.ts",
    "test-stay-alive": "ts-node allTests.ts --stay-alive",
    "test-write-results": "ts-node allTests.ts --write-results",
    "test-build-archive": "ts-node util/ddTest/buildArchive.ts",
    "test-write-and-archive": "yarn test-write-results; yarn test-build-archive",
    "repl": "ts-node apps/cmd/repl/main.ts",
    "build-dist": "tsc --jsx react --outDir dist --declaration --esModuleInterop apps/fiddle/main.tsx && tsc --outDir dist apps/cmd/repl/main.ts",
    "build-interp-bundle": "esbuild --bundle --outfile=dist/index.mjs --format=esm --target=es6 core/simple/interpreter.ts",
    "build-fp-ui": "esbuild apps/fp/ui/main.tsx --bundle --outfile=apps/fp/ui/public/bundle.js '--define:process.env.NODE_ENV=\"development\"' --loader:.dl=text --loader:.css=text --sourcemap",
    "serve-fp-ui": "cd apps/fp/ui/public && http-server -c-1",
    "build-fiddle": "esbuild apps/fiddle/main.tsx --bundle --outfile=apps/fiddle/public/bundle.js '--define:process.env.NODE_ENV=\"development\"' --loader:.dl=text --sourcemap",
    "serve-fiddle": "cd apps/fiddle/public && http-server -c-1",
    "build-notebook-ui": "esbuild apps/notebook/main.tsx --bundle --outfile=apps/notebook/public/bundle.js '--define:process.env.NODE_ENV=\"development\"' --sourcemap",
    "serve-notebook-ui": "cd apps/notebook/public && http-server -c-1",
    "build-actors-ui": "esbuild apps/actors/main.tsx --bundle --outfile=apps/actors/public/bundle.js --sourcemap '--define:process.env.NODE_ENV=\"development\"' --loader:.dl=text",
    "serve-actors-ui": "cd apps/actors/public && http-server -c-1",
    "build-parserlib-playground": "esbuild apps/parserlib/playground/main.tsx --bundle --outfile=apps/parserlib/playground/public/bundle.js '--define:process.env.NODE_ENV=\"development\"' --sourcemap",
    "serve-parserlib-playground": "cd apps/parserlib/playground/public && http-server -c-1",
    "build-ddtest-viewer": "esbuild util/ddTest/viewer/main.tsx --bundle --outfile=util/ddTest/viewer/public/bundle.js '--define:process.env.NODE_ENV=\"development\"' --sourcemap --external:path --external:fs --external:crypto",
    "serve-ddtest-viewer": "cd util/ddTest/viewer && http-server -c-1",
    "cloc": "cloc --vcs=git --exclude-dir=dist",
    "benchmark": "ts-node allBenchmarks.ts"
  },
  "devDependencies": {
    "@jaegertracing/plexus": "^0.1.0",
    "@types/diff": "^4.0.2",
    "@types/node": "^15.0.3",
    "@types/parsimmon": "^1.10.1",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "airtable": "^0.11.0",
    "aws-sdk": "^2.906.0",
    "diff": "^4.0.2",
    "esbuild": "^0.5.18",
    "http-server": "^0.12.3",
    "prettier": "^2.0.4",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2",
    "v8-profiler-node8": "^6.3.0"
  },
  "dependencies": {
    "@hookeasy/use-title": "^1.0.2",
    "@react-hook/window-size": "^3.0.7",
    "classnames": "^2.3.1",
    "graphviz-react": "^1.2.0",
    "immutable": "^4.0.0-rc.12",
    "parsimmon": "^1.13.0",
    "path-browserify": "^1.0.1",
    "prettier-printer": "^1.1.4",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.2",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^5.2.0",
    "react-select": "^4.3.1",
    "react-simple-code-editor": "https://github.com/vilterp/react-simple-code-editor#a1f205b84348964fba02163d1e52e908613d484b",
    "react-textarea-autosize": "^8.3.2",
    "react-use-elmish": "^1.0.0",
    "react-use-localstorage": "^3.4.1",
    "simple-markdown": "^0.7.3",
    "use-hash-param": "^0.2.3",
    "use-http": "^1.0.20",
    "w3c-hr-time": "^1.0.2"
  },
  "bin": {
    "datalog-ts": "dist/cmd/repl.js"
  },
  "files": [
    "dist"
  ]
}
